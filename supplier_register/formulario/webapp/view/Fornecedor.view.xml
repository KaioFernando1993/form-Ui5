<mvc:View controllerName="formulario.controller.Fornecedor" 
    xmlns="sap.m" 
    xmlns:uxap="sap.uxap" 
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns:forms="sap.ui.layout.form">
    <uxap:ObjectPageLayout 
        showTitleInHeaderContent="false" 
        showHeaderContent="true" 
        showEditHeaderButton="false" 
        showFooter="false" 
        class="sapUiSizeCompact">
        <!-- Header Titulo -->
        <uxap:headerTitle>
            <uxap:ObjectPageHeader 
                objectSubtitle="{i18n>namePage}" 
                isObjectIconAlwaysVisible="false" 
                isObjectTitleAlwaysVisible="true" 
                isObjectSubtitleAlwaysVisible="true" 
                showPlaceholder="true" 
                objectImageURI="sap-icon://supplier">
                <uxap:navigationBar>
                    <Bar>
                        <contentLeft>
                            <Button type="Back" press="_onFioriObjectPageHeaderPress"/>
                        </contentLeft>
                        <contentMiddle>
                            <Title text="{i18n>nameStore}"/>
                        </contentMiddle>
                    </Bar>
                </uxap:navigationBar>
                <!-- Icons -->
                <uxap:actions>
                    <Button 
                        text="{i18n>save}" 
                        icon="sap-icon://save" 
                        iconFirst="false" 
                        type="Emphasized" 
                        enabled="true"
                        press="onSave">
                        <layoutData>
                            <OverflowToolbarLayoutData moveToOverflow="true"/>
                        </layoutData>
                    </Button>
                    <!-- <Button 
                        icon="sap-icon://action" 
                        iconFirst="true" 
                        type="Transparent" 
                        enabled="true">
                        <layoutData>
                            <OverflowToolbarLayoutData moveToOverflow="true"/>
                        </layoutData>
                    </Button> -->
                </uxap:actions>
                <uxap:breadCrumbsLinks/>
            </uxap:ObjectPageHeader>
        </uxap:headerTitle>
        <!-- Icones -->
        <uxap:headerContent>
            <VBox>
                <items>
                    <core:Icon 
                        src="sap-icon://supplier" class="sapUxAPObjectPageHeaderObjectImage sapSmartTemplatesObjectPageHeaderImage" 
                        size="6rem" 
                        width="6rem" 
                        height="6rem" 
                        visible="true"/>
                </items>
            </VBox>
        </uxap:headerContent>
        <uxap:sections>
            <uxap:ObjectPageSection title="Adress">
                <uxap:subSections>
                    <uxap:ObjectPageSubSection title="{i18n>adress}">
                            <!-- Coluna 1 -->
                            <VBox 
                                alignItems="Stretch" 
                                direction="Column" 
                                fitContainer="false" 
                                width="710px" 
                                height="auto" 
                                justifyContent="SpaceBetween" 
                                renderType="Div" 
                                visible="true" 
                                displayInline="false">
                                <items>
                                    <VBox 
                                        class="sapUiSmallMarginBeginEnd" 
                                        alignItems="Stretch" 
                                        direction="Column" 
                                        fitContainer="false" 
                                        width="auto" 
                                        height="auto" 
                                        justifyContent="SpaceBetween" 
                                        renderType="Div" 
                                        visible="true" 
                                        displayInline="false">
                                        <items>
                                            <Label 
                                                text="{i18n>street}" 
                                                design="Standard" 
                                                width="100%" 
                                                required="false" 
                                                textAlign="Begin" 
                                                textDirection="Inherit" 
                                                visible="true"/>
                                            <Input
                                                value="{ObjectModel>/street}"
                                                type="Text" 
                                                placeholder="{i18n>street}" 
                                                showValueHelp="false" 
                                                enabled="true" 
                                                visible="true" 
                                                width="40%" 
                                                valueHelpOnly="false" 
                                                required="false" 
                                                valueStateText="Invalid entry" />
                                        </items>
                                        <layoutData/>
                                    </VBox>
                                    <VBox 
                                        class="sapUiSmallMarginBeginEnd" 
                                        alignItems="Stretch" 
                                        direction="Column" 
                                        fitContainer="false" 
                                        width="auto" 
                                        height="auto" 
                                        justifyContent="SpaceBetween" 
                                        renderType="Div" 
                                        visible="true" 
                                        displayInline="false">
                                        <items>
                                            <Label 
                                                text="{i18n>neighborhood}" 
                                                design="Standard" 
                                                width="100%" 
                                                required="false" 
                                                textAlign="Begin" 
                                                textDirection="Inherit" 
                                                visible="true"/>
                                            <Input 
                                                value="{ObjectModel>/neighborhood}"
                                                placeholder="{i18n>neighborhood}" 
                                                type="Text" 
                                                showValueHelp="false" 
                                                enabled="true" 
                                                visible="true" 
                                                width="40%" 
                                                valueHelpOnly="false" 
                                                required="false" valueStateText="Invalid entry" />
                                        </items>
                                        <layoutData/>
                                    </VBox>
                                    <VBox 
                                        class="sapUiSmallMarginBeginEnd" 
                                        alignItems="Stretch" 
                                        direction="Column" 
                                        fitContainer="false" 
                                        width="auto" 
                                        height="auto" 
                                        justifyContent="SpaceBetween" 
                                        renderType="Div" 
                                        visible="true" 
                                        displayInline="false">
                                        <items>
                                            <Label 
                                                text="{i18n>city}" 
                                                design="Standard" 
                                                width="100%" 
                                                required="false" 
                                                textAlign="Begin" 
                                                textDirection="Inherit" 
                                                visible="true"/>
                                            <Input 
                                                value="{ObjectModel>/city}" 
                                                placeholder="{i18n>city}" 
                                                type="Text" 
                                                showValueHelp="false" 
                                                enabled="true" 
                                                visible="true" 
                                                width="40%" 
                                                valueHelpOnly="false" 
                                                required="false"
                                                valueStateText="Invalid entry" />
                                        </items>
                                        <layoutData/>
                                    </VBox>
                                    <VBox 
                                        class="sapUiSmallMarginBeginEnd" 
                                        alignItems="Stretch" 
                                        direction="Column" 
                                        fitContainer="false" 
                                        width="auto" 
                                        height="auto" 
                                        justifyContent="SpaceBetween" 
                                        renderType="Div" 
                                        visible="true" 
                                        displayInline="false">
                                        <items>
                                            <Label 
                                                text="{i18n>cellPhone}" 
                                                design="Standard" 
                                                width="100%" 
                                                required="false" 
                                                textAlign="Begin" 
                                                textDirection="Inherit" 
                                                visible="true"/>
                                            <Input 
                                                value="{ObjectModel>/cellPhone}" 
                                                placeholder="{i18n>cellPhone}" 
                                                type="Text" 
                                                showValueHelp="false" 
                                                enabled="true" 
                                                visible="true" 
                                                width="40%" 
                                                valueHelpOnly="false" 
                                                required="false" 
                                                valueStateText="Invalid entry" />
                                            <Button text="Celular" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                                        </items>
                                        <layoutData/>
                                    </VBox>
                                    <VBox 
                                        class="sapUiSmallMarginBeginEnd" 
                                        alignItems="Stretch" 
                                        direction="Column" 
                                        fitContainer="false" 
                                        width="auto" 
                                        height="auto" 
                                        justifyContent="SpaceBetween" 
                                        renderType="Div" 
                                        visible="true" 
                                        displayInline="false">
                                        <items>
                                            <Label 
                                                text="{i18n>email}" 
                                                design="Standard" 
                                                width="100%" 
                                                required="false" 
                                                textAlign="Begin" 
                                                textDirection="Inherit" 
                                                visible="true"/>
                                            <Input 
                                                value="{ObjectModel>/email}" 
                                                placeholder="{i18n>email}" 
                                                type="Text" 
                                                showValueHelp="false" 
                                                enabled="true" 
                                                visible="true" 
                                                width="40%" 
                                                valueHelpOnly="false" 
                                                required="false" 
                                                valueStateText="Invalid entry" />
                                            <Button text="Email" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                                        </items>
                                        <layoutData/>
                                    </VBox>
                                </items>
                            </VBox>

                            <!-- Coluna 2 -->
                            <VBox 
                                alignItems="Stretch" 
                                direction="Column" 
                                fitContainer="false" 
                                width="710px" 
                                height="auto" 
                                justifyContent="SpaceBetween" 
                                renderType="Div" 
                                visible="true" 
                                displayInline="false">

                                    <VBox 
                                        class="sapUiSmallMarginBeginEnd" 
                                        alignItems="Stretch" 
                                        direction="Column" 
                                        fitContainer="false" 
                                        width="auto" 
                                        height="auto" 
                                        justifyContent="SpaceBetween" 
                                        renderType="Div" 
                                        visible="true" 
                                        displayInline="false">
                                        <items>
                                            <Label 
                                                text="{i18n>telephone}" 
                                                design="Standard" 
                                                width="100%" 
                                                required="false" 
                                                textAlign="Begin" 
                                                textDirection="Inherit" 
                                                visible="true"/>
                                            <Input 
                                                value="{ObjectModel>/phoneNumber}" 
                                                placeholder="{i18n>telephone}" 
                                                type="Text" 
                                                showValueHelp="false" 
                                                enabled="true" 
                                                visible="true" 
                                                width="40%" 
                                                valueHelpOnly="false" 
                                                required="false" 
                                                valueStateText="Invalid entry" />
                                        </items>
                                        <layoutData/>
                                    </VBox>
                                    <VBox 
                                        class="sapUiSmallMarginBeginEnd" 
                                        alignItems="Stretch" 
                                        direction="Column" 
                                        fitContainer="false" 
                                        width="auto" 
                                        height="auto" 
                                        justifyContent="SpaceBetween" 
                                        renderType="Div" 
                                        visible="true" 
                                        displayInline="false">
                                        <items>
                                            <Label 
                                                text="{i18n>fax}" 
                                                design="Standard" 
                                                width="100%" 
                                                required="false" 
                                                textAlign="Begin" 
                                                textDirection="Inherit" 
                                                visible="true"/>
                                            <Input 
                                                value="{ObjectModel>/Fax}" 
                                                placeholder="{i18n>fax}" 
                                                type="Text" 
                                                showValueHelp="false" 
                                                enabled="true" 
                                                visible="true" 
                                                width="40%" 
                                                valueHelpOnly="false" 
                                                required="false" 
                                                valueStateText="Invalid entry"/>
                                        </items>
                                        <layoutData/>
                                    </VBox>
                                    <VBox 
                                        class="sapUiSmallMarginBeginEnd" 
                                        alignItems="Stretch" 
                                        direction="Column" 
                                        fitContainer="false" 
                                        width="auto" 
                                        height="auto" 
                                        justifyContent="SpaceBetween" 
                                        renderType="Div" 
                                        visible="true" 
                                        displayInline="false">
                                        <items>
                                            <Label 
                                                text="{i18n>number}" 
                                                design="Standard" 
                                                width="100%" 
                                                required="false" 
                                                textAlign="Begin" 
                                                textDirection="Inherit" 
                                                visible="true"/>
                                            <Input  
                                                value="{ObjectModel>/number}" 
                                                placeholder="{i18n>number}" 
                                                type="Text" 
                                                showValueHelp="false" 
                                                enabled="true" 
                                                visible="true" 
                                                width="40%" 
                                                valueHelpOnly="false" 
                                                required="false" 
                                                valueStateText="Invalid entry" />
                                        </items>
                                        <layoutData/>
                                    </VBox>
                                    <VBox 
                                        class="sapUiSmallMarginBeginEnd" 
                                        alignItems="Stretch" 
                                        direction="Column" 
                                        fitContainer="false" 
                                        width="auto" 
                                        height="auto" 
                                        justifyContent="SpaceBetween" 
                                        renderType="Div" 
                                        visible="true" 
                                        displayInline="false">
                                        <items>
                                            <Label 
                                                text="{i18n>zipCode}" 
                                                design="Standard" 
                                                width="100%" 
                                                required="false" 
                                                textAlign="Begin" 
                                                textDirection="Inherit" 
                                                visible="true"/>
                                            <Input 
                                                value="{ObjectModel>/zipCode}" 
                                                placeholder="{i18n>zipCode}" 
                                                type="Text" 
                                                showValueHelp="false" 
                                                enabled="true" 
                                                visible="true" 
                                                width="40%" 
                                                alueHelpOnly="false" 
                                                required="false" 
                                                valueStateText="Invalid entry" />
                                        </items>
                                        <layoutData/>
                                    </VBox>
                                <layoutData/>
                            </VBox>

                            <!-- Coluna 3 -->
                            <VBox 
                                alignItems="Stretch" 
                                direction="Column" 
                                fitContainer="false" 
                                width="auto" 
                                height="auto" 
                                justifyContent="SpaceBetween" 
                                renderType="Div" 
                                visible="true" 
                                displayInline="false">
                                <items>
                                    <VBox 
                                        class="sapUiSmallMarginBeginEnd" 
                                        alignItems="Stretch" 
                                        direction="Column" 
                                        fitContainer="false" 
                                        width="auto" 
                                        height="auto" 
                                        justifyContent="SpaceBetween" 
                                        renderType="Div" 
                                        visible="true" 
                                        displayInline="false">
                                        <items>
                                            <Label 
                                                text="{i18n>region}" 
                                                design="Standard" 
                                                width="100%" 
                                                required="false" 
                                                textAlign="Begin" 
                                                textDirection="Inherit" 
                                                visible="true"/>
                                            <Input 
                                                value="{ObjectModel>/region}" 
                                                placeholder="{i18n>region}" 
                                                type="Text" 
                                                showValueHelp="false" 
                                                enabled="true" 
                                                visible="true" 
                                                width="60%" 
                                                valueHelpOnly="false" 
                                                required="false" 
                                                valueStateText="Invalid entry" />
                                        </items>
                                        <layoutData/>
                                    </VBox>
                                    <VBox 
                                        class="sapUiSmallMarginBeginEnd" 
                                        alignItems="Stretch" 
                                        direction="Column" 
                                        fitContainer="false" 
                                        width="auto" 
                                        height="auto" 
                                        justifyContent="SpaceBetween" 
                                        renderType="Div" 
                                        visible="true" 
                                        displayInline="false">
                                        <items>
                                            <Label 
                                                text="{i18n>extetion}" 
                                                design="Standard" 
                                                width="100%" 
                                                required="false" 
                                                textAlign="Begin" 
                                                textDirection="Inherit" 
                                                visible="true"/>
                                            <Input 
                                                value="{ObjectModel>/extensionOne}" 
                                                placeholder="{i18n>extetion}" 
                                                type="Text" 
                                                showValueHelp="false" 
                                                enabled="true" 
                                                visible="true" 
                                                width="60%" 
                                                valueHelpOnly="false" 
                                                required="false" 
                                                valueStateText="Invalid entry" />
                                            <Button 
                                                text="Telefone" 
                                                type="Default" 
                                                iconFirst="true" 
                                                width="auto" 
                                                enabled="true" 
                                                visible="true" 
                                                iconDensityAware="false"/>
                                        </items>
                                        <layoutData/>
                                    </VBox>
                                    <VBox 
                                        class="sapUiSmallMarginBeginEnd" alignItems="Stretch" 
                                        direction="Column" 
                                        fitContainer="false" 
                                        width="auto" 
                                        height="auto" 
                                        justifyContent="SpaceBetween" 
                                        renderType="Div" 
                                        visible="true" 
                                        displayInline="false">
                                        <items>
                                            <Label 
                                                text="{i18n>extetion}" 
                                                design="Standard" 
                                                width="100%" 
                                                required="false" 
                                                textAlign="Begin" 
                                                textDirection="Inherit" 
                                                visible="true"/>
                                            <Input 
                                                value="{ObjectModel>/extensionTwo}" 
                                                placeholder="{i18n>extetion}" 
                                                type="Text" 
                                                showValueHelp="false" 
                                                enabled="true" 
                                                visible="true" 
                                                width="60%" 
                                                valueHelpOnly="false" 
                                                required="false" 
                                                valueStateText="Invalid entry" />
                                            <Button 
                                                text="{i18n>cellPhone}" 
                                                type="Default" 
                                                iconFirst="true" 
                                                width="auto" 
                                                enabled="true" 
                                                visible="true" 
                                                iconDensityAware="false"/>
                                        </items>
                                        <layoutData/>
                                    </VBox>
                                </items>
                                <layoutData/>
                            </VBox>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>
            <!-- Assinatura Eletronica -->
            <uxap:ObjectPageSection title="Assinatura eletroinca">
                <uxap:subSections>
                    <uxap:ObjectPageSubSection title="{i18n>signature}">
                        <uxap:blocks>
                            <Table 
                                width="100%" 
                                growing="true" 
                                growingScrollToLoad="true" 
                                growingThreshold="25" 
                                items="{ObjectModel>/supplier}" 
                                id="tableId">
                                <headerToolbar>
                                    <OverflowToolbar design="Transparent">
                                        <content>
                                            <ToolbarSpacer/>
                                            <!-- Inserir Linha -->
                                            <Button 
                                                text="{i18n>insert}" 
                                                press="onInsertSupplier" icon="sap-icon://add" 
                                                iconFirst="false" 
                                                type="Transparent">
                                                <layoutData>
                                                    <OverflowToolbarLayoutData moveToOverflow="true"/>
                                                </layoutData>
                                            </Button>
                                            <!-- Excluir Linha -->
                                            <Button 
                                                text="{i18n>delete}"
                                                press="onRemoveSupplier"
                                                icon="sap-icon://delete" iconFirst="false" 
                                                type="Reject">
                                                <layoutData>
                                                    <OverflowToolbarLayoutData moveToOverflow="true"/>
                                                </layoutData>
                                            </Button>
                                            <!-- Configurações -->
                                            <Button 
                                                icon="sap-icon://action-settings"   
                                                type="Transparent">
                                                <layoutData>
                                                    <OverflowToolbarLayoutData moveToOverflow="true"/>
                                                </layoutData>
                                            </Button>
                                        </content>
                                    </OverflowToolbar>
                                </headerToolbar>
                                <!-- Colunas das Tabelas -->
                                <columns>
                                    <Column 
                                        width="auto" 
                                        minScreenWidth="Tablet" 
                                        demandPopin="true">
                                        <header>
                                            <Text text="{i18n>select}" width="auto"/>
                                        </header>
                                    </Column>
                                    <Column 
                                        width="auto" 
                                        minScreenWidth="Tablet"
                                        demandPopin="true">
                                        <header>
                                            <Text text="{i18n>name}" width="auto"/>
                                        </header>
                                    </Column>
                                    <Column width="auto" minScreenWidth="Tablet" demandPopin="true">
                                        <header>
                                            <Text text="{i18n>cpf}" width="auto"/>
                                        </header>
                                    </Column>
                                </columns>
                                <!-- Linha da Tabela -->
                                <items>
                                    <ColumnListItem 
                                        type="Active">
                                        <cells>
                                            <CheckBox 
                                                selected="{ObjectModel>selected}" />
                                                <Input 
                                                    type="text"
                                                    value="{ObjectModel>name}" 
                                                    width="60%"/>
                                                <Input 
                                                    type="number" 
                                                    value="{ObjectModel>cpf}" placeholder="  .   .   -  " 
                                                    width="60%"/>
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>
        </uxap:sections>
    </uxap:ObjectPageLayout>
</mvc:View>
